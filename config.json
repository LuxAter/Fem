{
  "CXX": "clang++",
  "CC": "clang",
  "CCIGNORE": "",
  "CXXIGNORE": "",
  "CCFLAGS": ["-fPIC -Wall -Wpedantic --static"],
  "CXXFLAGS": ["-std=c++17", "-fPIC", "-Wall", "-Wpedantic", "--static", "-O3"],
  "LINK": "",
  "source": "src",
  "include_dir": "include",
  "include": [],
  "build": "build",
  "fem": {
    "path": "fem",
    "files": "src/main.cpp",
    "dep": ["libfem.a"],
    "group": "main",
    "type": "exe"
  },
  "libfem.a": {
    "path": "$(BUILD)/libfem.a",
    "files_exclude": "src/main.cpp",
    "files": "src/*.cpp",
    "type": "lib"
  }
}
